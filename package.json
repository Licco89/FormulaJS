{
	"name": "formula-js",
	"version": "0.0.3",
	"description": "Generate formulas on the fly",
	"main": "formula.min.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"preversion": "jsdoc . -R README.md -c docs/jsdoc-conf.json && git add * || true"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Zenoo/FormulaJS.git"
	},
	"keywords": [
		"formula",
		"generator",
		"custom",
		"math"
	],
	"author": "Zenoo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Zenoo/FormulaJS/issues"
	},
	"homepage": "https://github.com/Zenoo/FormulaJS#readme",
	"dependencies": {
		"@babel/polyfill": "^7.2.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "usage"
				}
			]
		],
		"minified": true,
		"comments": false,
		"ignore": [
			"**/*.min.js"
		]
	}
}
